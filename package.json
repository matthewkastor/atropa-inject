{
  "name": "atropa-inject",
  "version": "2014.2.1",
  "description": "Contains tools for injecting elements and assemblies into web pages.",
  "main": "./src/atropa-inject.js",
  "directories": {
    "lib": "src",
    "doc": "docs"
  },
  "devDependencies": {
    "atropa-jsformatter": ">=0.1.2",
    "atropa-jslint": ">=0.1.2",
    "browserify": ">=2.29.1",
    "jasmine-node": ">=1.11.0",
    "jsdoc-toolkit": "git+https://github.com/matthewkastor/node-jsdoc-toolkit.git#fix-tests",
    "atropa-server": "git+https://github.com/matthewkastor/atropa-server.git#master"
  },
  "scripts": {
    "start": "node server.js",
    "test": "jasmine-node specs/",
    "buildBrowserModule": "node dev/browserify.js",
    "buildDocs": "jsdoc-toolkit -v -d=./docs/jsdoc/ -r -s node_modules/atropa-header/src/ src/ node_modules/atropa-setAsOptionalArg/src/",
    "lint": "atropa-jslint ./src/atropa-inject.js",
    "srcFormat": "atropa-jsformatter ./src/atropa-inject.js ./src/atropa-inject.js",
    "prestart": "node dev/browserify.js",
    "pretest": "node dev/browserify.js",
    "buildVsdocs": "jsdoc-toolkit -v -d=./docs/vsdoc/ -t=./node_modules/jsdoc-toolkit/templates/vsdoc/ -r -s node_modules/atropa-header/src/ src/ node_modules/atropa-setAsOptionalArg/src/"
  },
  "homepage": "https://github.com/matthewkastor/atropa-inject/",
  "bugs": "https://github.com/matthewkastor/atropa-inject/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/matthewkastor/atropa-inject.git"
  },
  "keywords": [
    "atropa-inject",
    "atropa"
  ],
  "author": {
    "name": "Matthew Kastor",
    "email": "matthewkastor@gmail.com",
    "url": "https://plus.google.com/100898583798552211130"
  },
  "licenses": [
    {
      "type": "gpl-3.0",
      "url": "http://www.gnu.org/licenses/gpl-3.0-standalone.html"
    }
  ],
  "dependencies": {
    "atropa-header": "git+https://github.com/matthewkastor/atropa-header.git#master",
    "atropa-setAsOptionalArg": "git+https://github.com/matthewkastor/atropa-setAsOptionalArg.git#master"
  }
}
